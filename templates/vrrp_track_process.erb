vrrp_track_process <%= @name %> {
  process    "<%= @proc_name %>"
  <%- if @weight -%>
  weight    <%= @weight %>
  <%- end -%>
  <%- if @quorum -%>
  quorum    <%= @quorum %>
  <%- end -%>
  <%- if @delay -%>
  delay     <%= @delay %>
  <%- end -%>
  <%- if @fullcommand -%>
  fullcommand
  <%- end -%>
}
